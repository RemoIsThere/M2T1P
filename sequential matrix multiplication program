#include <iostream>
#include <fstream>
#include <cstdlib>
#include <ctime>

using namespace std;

int main() {
    const int N = 10; // size of matrices
    int A[N][N], B[N][N], C[N][N]; // matrices

    // initialize random seed
    srand(time(NULL));

    // initialize matrices A and B with random values
    for (int i = 0; i < N; i++) {
        for (int j = 0; j < N; j++) {
            A[i][j] = rand() % 10;
            B[i][j] = rand() % 10;
        }
    }

    // perform matrix multiplication
    for (int i = 0; i < N; i++) {
        for (int j = 0; j < N; j++) {
            C[i][j] = 0;
            for (int k = 0; k < N; k++) {
                C[i][j] += A[i][k] * B[k][j];
            }
        }
    }

    // write output to file
    ofstream outfile("output.txt");
    for (int i = 0; i < N; i++) {
        for (int j = 0; j < N; j++) {
            outfile << C[i][j] << " ";
        }
        outfile << endl;
    }
    outfile.close();

    return 0;
}
